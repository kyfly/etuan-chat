{
  "name": "ChatRoom",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "type": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "msg": {
      "type": [
        {
          "created": {
            "type": "date",
            "required": true
          },
          "msgType": {
            "type": "string",
            "required": true
          },
          "msgContent": {
            "type": "string",
            "required": true
          }
        }
      ]
    }
  },
  "validations": [],
  "relations": {
    "chatGroups": {
      "type": "embedsMany",
      "model": "ChatGroup",
      "option": {
        "validate": true,
        "autoId": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
